<table class="table table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    {{#each users}}
        <tr data-id="{{id}}">
            <td>{{firstName}}</td>
            <td>{{email}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>